{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver", 
      { 
        "root": "./src",
        "alias": {
          "@routes": "./src/routes/index",
          "@variables": "./src/config/variables",
          "@mappers": "./src/utils/mappers",
          "@api": "./src/config/api_request"
        }
      }
    ]
  ]
}